<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/twitter-bootstrap/4.1.0/css/bootstrap.min.css">
	<link rel='stylesheet' href='/stylesheets/style.css' />
	<style>
		.img1 {
		width: 60%;
		height: 80%;
		}
	</style>
  </head>
  <body>
  	<div class="container">
  		<div class="row">
  			<div class="col-md-12">
   		 		<h1 class="text-center"><%= title %></h1>  
   		 	</div>  
   		</div>
	</div>

	<div class="row">

		<div class=col-md-6>

			<h1 class="text-center"><%= website[0]%></h1><!-- aliexpress-->
			<% listingsAliex.forEach(function(listing){ %>
			<a href="<%=listing.url%>">
				<div class="col-md-12">
				<div class="card text-center bg-light">
					<div class="card-head">
						<img src="<%=listing.img%>" class="img1">
					</div>
					<div class="card-body">						
						<h6><%=listing.title%></h6>
						<h6><%=listing.price%></h6>		
					</div>
				</div>
				</div>
			</a>
			<% }) %>
		</div>
		
		<div class=col-md-6>
				<h1 class="text-center"><%= website[1]%></h1><!-- Ebay -->
			<% listingsEbay.forEach(function(listing){ %>
		    <a href="<%=listing.url%>">
			<div class="col-md-12">
				<div class="card text-center bg-light">
					<div class="card-head">
						<img src="<%=listing.img%>" >
					</div>
					<div class="card-body">
						<h6><%=listing.title%></h6>
						<h6><%=listing.price%></h6>		
					</div>
				</div>
			</div>
			</a>
			<% }) %>
		</div>


	</div>
  </body>
</html>
